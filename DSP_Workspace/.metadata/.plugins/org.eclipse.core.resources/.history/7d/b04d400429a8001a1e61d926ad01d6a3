            fft_draft[2*k+1+8] = temp2 >> 8;
        }
    }
    //
    for (p=0;p<2;p++)
    {
        for (k=p;k<16;k=k+4)
        {
            a_r = fft_draft[2*k];
            a_i = fft_draft[2*k+1];
