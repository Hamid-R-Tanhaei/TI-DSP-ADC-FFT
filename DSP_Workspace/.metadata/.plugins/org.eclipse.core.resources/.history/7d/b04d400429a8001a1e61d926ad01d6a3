        { temp3 = 100;}
        if (temp1 > 1500) // 125mV
        { temp3 = 125;}
        if (temp1 > 2150) // 150mV
        { temp3 = 150;}
        if (temp1 > 3000) // 175mV
        { temp3 = 175;}
